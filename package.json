{
  "name": "vanitycr",
  "version": "1.0.0",
  "type": "module",
  "bin": "./dist/main.js",
  "exports": "./dist/index.js",
  "devDependencies": {
    "@types/node": "latest",
    "bun": "latest",
    "deno": "latest",
    "execa": "^9.6.0",
    "npm-run-all2": "^8.0.4",
    "peggy": "^5.0.6",
    "typescript": "latest",
    "wrangler": "latest"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.4",
    "@hono/zod-validator": "^0.7.3",
    "hono": "^4.9.8",
    "hono-alibaba-cloud-fc3-adapter": "^1.0.2",
    "micro-memoize": "^4.2.0",
    "zod": "^4.1.11"
  },
  "scripts": {
    "build:peggy": "node ./build-peggy.ts",
    "build:tsc": "tsc --noCheck",
    "build": "run-s build:peggy build:tsc",
    "lint": "run-s build:peggy _lint:tsc",
    "_lint:tsc": "tsc --noEmit",
    "start": "run-s build _start:node",
    "_start:node": "node ./dist/main.js",
    "dev": "run-s build:peggy _dev:node",
    "_dev:node": "node --watch ./src/main.ts",
    "dev:deno": "deno serve -A ./src/index.ts",
    "dev:bun": "bun ./src/index.ts",
    "dev:workerd": "wrangler dev"
  }
}
